<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4"
	xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
	http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">


  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Modispatcher</servlet-name>
    <servlet-class>com.mobilink.SubProcess.Modispatcher</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>Modispatcher</servlet-name>
    <url-pattern>/Modispatcher</url-pattern>
  </servlet-mapping>

  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ContentDownload</servlet-name>
    <servlet-class>com.mobilink.SubProcess.ContentDownload</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ContentDownload</servlet-name>
    <url-pattern>/CD</url-pattern>
  </servlet-mapping>

    <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.mobilink.SubProcess.Login</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>

</web-app>
